#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/amydutton/Sites/redwood/create-rwsdk/node_modules/.pnpm/tiged@2.12.7/node_modules/tiged/node_modules:/Users/amydutton/Sites/redwood/create-rwsdk/node_modules/.pnpm/tiged@2.12.7/node_modules:/Users/amydutton/Sites/redwood/create-rwsdk/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/amydutton/Sites/redwood/create-rwsdk/node_modules/.pnpm/tiged@2.12.7/node_modules/tiged/node_modules:/Users/amydutton/Sites/redwood/create-rwsdk/node_modules/.pnpm/tiged@2.12.7/node_modules:/Users/amydutton/Sites/redwood/create-rwsdk/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin.js" "$@"
else
  exec node  "$basedir/../../bin.js" "$@"
fi
